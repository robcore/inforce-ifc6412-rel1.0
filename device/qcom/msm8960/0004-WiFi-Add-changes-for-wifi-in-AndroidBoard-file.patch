 LOCAL_MODULE_TAGS  := optional
@@ -112,7 +120,7 @@ $(shell mkdir -p $(TARGET_OUT_ETC)/firmware/wlan/prima; \
 	ln -sf /data/misc/wifi/WCNSS_qcom_cfg.ini \
 	$(TARGET_OUT_ETC)/firmware/wlan/prima/WCNSS_qcom_cfg.ini)
 
-endif
+#endif
 
 include $(CLEAR_VARS)
